{
  "expo": {
    "name": "kaizen-ride",
    "slug": "kaizen-ride",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/app-icon2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/app-icon2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vanvince.kaizenride"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon2.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vanvince.kaizenride",
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCfARqjJgmhmIp2XDWlYnvv63x0rxvLFsA"
        }
      },
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/app-icon2.png"
    },
    "extra": {
      "eas": {
        "projectId": "fa4cb6c4-700e-42d1-893e-5ad9415ee3e3"
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(kaizen-ride) to use your location."
        }
      ],
      [
        "expo-notifications",
        {
          // "icon": "./local/assets/notification-icon.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/ambient.mp3",
            "./assets/action.mp3"
          ]
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Let this app access your photos."
        }
      ]
    ],
    "scheme": "com.vanvince.kaizenride"
  }
}
